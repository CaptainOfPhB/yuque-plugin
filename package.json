{
  "name": "yuque-plugin",
  "sideEffects": true,
  "version": "1.0.0",
  "description": "A Chrome extension that make Yuque more powerful",
  "scripts": {
    "start": "webpack --config scripts/webpack.config.dev.js",
    "build": "rm -rf build && webpack --config scripts/webpack.config.prod.js",
    "lint": "eslint --fix src --ext .ts,.tsx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*{.ts,.tsx}": [
      "yarn run lint",
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/chrome": "^0.0.133",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "6.4.1",
    "css-loader": "^5.1.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "4.5.2",
    "husky": "^5.1.3",
    "less": "^4.1.1",
    "less-loader": "7.3.0",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.3.9",
    "postcss-loader": "4.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "ts-import-plugin": "^1.6.7",
    "ts-loader": "^8.0.17",
    "typescript": "^4.2.3",
    "url-loader": "^4.1.1",
    "webpack": "4.46.0",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@types/turndown": "^5.0.0",
    "antd": "^4.14.1",
    "axios": "^0.21.1",
    "markmap-lib": "^0.11.6",
    "markmap-view": "^0.2.6",
    "noty": "3.1.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "turndown": "^7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaptainOfPhB/yuque-plugin.git"
  },
  "keywords": [
    "yuque",
    "chrome",
    "chrome-extension"
  ],
  "author": "CaptainOfPhB",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CaptainOfPhB/yuque-plugin/issues"
  },
  "homepage": "https://github.com/CaptainOfPhB/yuque-plugin#readme"
}
